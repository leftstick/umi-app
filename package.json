    "@umijs/plugin-locale": "^0.7.0",
    "@umijs/plugin-model": "^2.3.1",
    "@umijs/plugin-request": "^2.3.2",
    "@umijs/test": "^3.2.2",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.6",
    "react-coding-style": "^1.1.0",
    "umi": "^3.2.2",
    "umi-plugin-antd-theme": "^2.1.2"
